<%= form_with(model: user, local: true) do |form| %>
  

  <div class="field">
    <%= form.label :Nombre %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :Apellido %>
    <%= form.text_field :lastname %>
  </div>


  

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
